{
	"parser": "babel-eslint",
	"plugins": ["eslint:recommended", "react:recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true,
			"modules": true
		}
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base"],
	"env": {
		"jest": true
	},
	"rules": {
		"arrow-body-style": "warn"
	}
}
